
 
<div class="container-fluid bg-3 text-center">    
  <h3>Favorites</h3><br>
  <div class="row articles">
    {{#each articles}}
    <div class="col-sm-3"> 
      <a href="{{link}}">
          <img src="{{image}}" style="width:100%"/>
          <p class="category">{{category}}</p>
          <h4 class="title">{{title}}</h4>
          <a class="add-note btn btn-default" href="#" role="button">Add Note</a>
          <a class="delete btn btn-default" href="#" role="button">Delete</a>
      </a>
    </div>
    {{/each}}
  </div>
</div><br>

